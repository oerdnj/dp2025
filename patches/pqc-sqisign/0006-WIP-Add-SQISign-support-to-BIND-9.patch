 			case DST_ALG_ED25519:
 			case DST_ALG_ED448:
+			case DST_ALG_SQISIGN:
 				break;
 			default:
 				fatal("algorithm %s is incompatible with NSEC3"
@@ -324,6 +327,7 @@ keygen(keygen_ctx_t *ctx, isc_mem_t *mctx, int argc, char **argv) {
