 		ctx->size = 456;
 		break;
+	case DST_ALG_SQISIGN:
+		ctx->size = 512;
+		break;
 	}
 
 	if ((ctx->options & DST_TYPE_KEY) == 0) {
@@ -515,6 +522,7 @@ keygen(keygen_ctx_t *ctx, isc_mem_t *mctx, int argc, char **argv) {
