+				rlen = read(f, seed, len);
+				if (rlen < 0) {
+					if (errno == EINTR) {
+						continue;
+					}
+					break;
+				}
+				seed = (uint8_t *)seed + rlen;
+				len -= (size_t)rlen;
+			}
+			close(f);
+			if (len == 0) {
+				return 1;
+			}
+		}
+	}
+#endif
+#if FALCON_RAND_WIN32
+	{
+		HCRYPTPROV hp;
+
+		if (CryptAcquireContext(&hp, 0, 0, PROV_RSA_FULL,
+			CRYPT_VERIFYCONTEXT | CRYPT_SILENT))
+		{
+			BOOL r;
+
+			r = CryptGenRandom(hp, (DWORD)len, seed);
+			CryptReleaseContext(hp, 0);
+			if (r) {
