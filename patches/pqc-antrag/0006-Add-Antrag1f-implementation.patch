+/* see inner.h */
+int
+Zf(get_seed)(void *seed, size_t len)
+{
+	(void)seed;
+	if (len == 0) {
+		return 1;
+	}
+#if FALCON_RAND_GETENTROPY
+	if (getentropy(seed, len) == 0) {
+		return 1;
+	}
+#endif
+#if FALCON_RAND_URANDOM
+	{
+		int f;
+
+		f = open("/dev/urandom", O_RDONLY);
+		if (f >= 0) {
+			while (len > 0) {
+				ssize_t rlen;
