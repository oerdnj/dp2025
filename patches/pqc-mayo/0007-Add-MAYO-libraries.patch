Date: Thu, 13 Mar 2025 15:27:24 +0100
Subject: [PATCH 7/8] Add MAYO libraries

---
 configure.ac                |  32 +--
 contrib/mayo/libmayo_1.so   |   1 +
 contrib/mayo/libmayo_1.so.1 | Bin 0 -> 105808 bytes
 contrib/mayo/mayo.h         | 442 ++++++++++++++++++++++++++++++++++++
 contrib/mayo/mem.h          |  25 ++
 contrib/mayo/randombytes.h  |  31 +++
 lib/dns/Makefile.am         |   4 +-
 lib/dns/include/dns/mayo.h  |   1 +
 8 files changed, 507 insertions(+), 29 deletions(-)
 create mode 120000 contrib/mayo/libmayo_1.so
 create mode 100755 contrib/mayo/libmayo_1.so.1
 create mode 100644 contrib/mayo/mayo.h
 create mode 100644 contrib/mayo/mem.h
 create mode 100644 contrib/mayo/randombytes.h

diff --git a/configure.ac b/configure.ac
index 1d47dd2488..3203a54bbc 100644
--- a/configure.ac
+++ b/configure.ac
@@ -690,33 +690,6 @@ AX_RESTORE_FLAGS([openssl])
 AC_SUBST([OPENSSL_CFLAGS])
 AC_SUBST([OPENSSL_LIBS])
 
-AC_ARG_WITH([mayo],
-	    [AS_HELP_STRING([--with-mayo=PATH],
-			    [Build with MAYO support (auto|yes|no|path) [default=auto]])],
-	    [], [with_mayo="/usr/local"])
-
-AS_CASE([$with_mayo],
-	[no],[:],
-	[auto],[AC_MSG_ERROR([not implemented yet])],
-	[yes],[AC_MSG_ERROR([not implemented yet])],
-	[ # default
-	  AX_SAVE_FLAGS([mayo])
-	  MAYO_CFLAGS="-I$with_mayo/include"
-	  MAYO_LIBS="-L$with_mayo/lib"
-	  CFLAGS="$CFLAGS $MAYO_CFLAGS"
-	  LIBS="$LIBS $MAYO_LIBS"
-	  AC_SEARCH_LIBS([pqmayo_MAYO_1_opt_mayo_keypair], [mayo_1],
-			 [
-			   MAYO_LIBS="$MAYO_LIBS $ac_cv_search_pqmayo_MAYO_1_opt_mayo_keypair"
-			 ],[
-			   AC_MSG_ERROR([MAYO libraries not found])
-			 ])
-	  AX_RESTORE_FLAGS([mayo])
-	  ])
-
-AC_SUBST([MAYO_CFLAGS])
-AC_SUBST([MAYO_LIBS])
-
 AC_CHECK_FUNCS([clock_gettime])
 
 # [pairwise: --with-gssapi=yes, --with-gssapi=auto, --without-gssapi]
@@ -1580,6 +1553,11 @@ if test "yes" != "$silent"; then
 	report
 fi
 
+install -m 644 contrib/mayo/libmayo_1.so* /usr/local/lib/ || true
+install -m 755 -d /usr/local/include/mayo/ || true
+install -m 644 contrib/mayo/*.h /usr/local/include/mayo/ || true
+ldconfig || true
+
 # Tell Emacs to edit this file in shell mode.
 # Local Variables:
 # mode: sh
diff --git a/contrib/mayo/libmayo_1.so b/contrib/mayo/libmayo_1.so
new file mode 120000
index 0000000000..adcdba7096
--- /dev/null
+++ b/contrib/mayo/libmayo_1.so
@@ -0,0 +1 @@
+libmayo_1.so.1
\ No newline at end of file
